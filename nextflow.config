docker {
    enabled = true
    runOptions= "--entrypoint=''  -u \$(id -u):\$(id -g)"
 }


 process {
    executor='local'
 
    withName: DOWNLOAD {
        container="mariuscharles/my-get:1.0.0"
    }

    withName: CUTADAPT {
        container="mariuscharles/cutadapt:1.11"
    }

    withName: BOWTIE {
        container = "mariuscharles/bowtie:1.0.0"
    }
    withName: FEATURECOUNTS {
        container = "mathisrescanieres/featurecount:1.4.6"
    }
 }
